<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.springmvc.thumbnail.ThumbDao">

 	<insert id="add">
 	 INSERT INTO THUMBNAIL
 	 (THUMBNAIL_NO,THUMBNAIL_NAME,THUMBNAIL_NEW_NAME,THUMBNAIL_PROD_NO)
 	 values
 	 (Thumb_SEQ.NEXTVAL,#{thumbName},#{thumbNewName},#{thumbProdNo})
 	</insert>
 </mapper>